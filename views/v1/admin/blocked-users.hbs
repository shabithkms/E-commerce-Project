<div id="page-wrapper">
    <ol class="breadcrumb">
        <li class="pr-3"><a href="/admin" style="text-decoration: none;">Home</a></li>
        <li class="pr-3"> <a href="/admin/users" style="text-decoration: none;">Users</a></li>

        <li class="active">Blocked users</li>
    </ol>
    <div class="container">
        <table class="table table-striped mt-5" style="margin-top:30px;" id="adminTable">
            <thead>
                <tr>
                    <th scope="col">No.</th>
                    <th scope="col">Name</th>
                    <th scope="col">Email</th>
                    <th scope="col">mobile No</th>

                </tr>
            </thead>
            <tbody>
                {{#each blockedUsers}}
                <tr>
                    <td></td>
                    <td>{{this.firstname}} {{this.lastname}}</td>
                    <td>{{this.email}}</td>
                    <td>{{this.mobileNo}}</td>
                </tr>
                {{/each}}

            </tbody>
        </table>
    </div>
</div>
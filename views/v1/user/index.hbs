<main role="main">
    <!-- BEGIN Slider -->
    <div id="shopify-section-1514880862152" class="shopify-section">
        <div class="slider-area">
            <div class="slider-active owl-carousel">
                {{#each banners}}
                <div class="single-slider single-slider-hm1 bg-img"
                    style="background-image: url(https://shabith.ml/banners/{{this._id}}.jpg)">
                    <div class="container">
                        <div class="slider-content slider-content-style-1 slider-animated-1">

                            <p class="animated" style="color: #ffffff;">{{this.offer}}
                                <span></span>
                            </p>
                            <h2 class="animated" style="color: #ffffff;">{{this.bannerName}}</h2>

                            <div>
                                <p class="animated" style="color: #ffffff;">{{this.description}}</p>
                            </div>
                            <a class="btn-hover slider-btn-style animated" href="{{this.link}}">Shop Now</a>

                        </div>
                    </div>
                </div>
                {{/each}}
            </div>
        </div>
        <style>
            .slider-btn-style {
                border: 2px solid #ffffff;
                color: #ffffff;
            }

            .slider-content-style-1>h3 span {
                color: #a8741a;
            }

            .slider-btn-style:hover {
                border: 2px solid #a8741a;
                color: #ffffff;
            }
        </style>
    </div>
    {{!-- Product Section --}}
    <div id="shopify-section-1515045233374" class="shopify-section">
        <div class="product-area neha_prod_area">
            <div class="container">
                <div class="product-style">
                    <div class="product-tab-list mb-45 nav product-menu-mrg" id="mens" role="tablist">

                        <a class="" href="/categoryProducts/{{firstC}}">
                            <h4>SHOP BY CATEGORY</h4>
                        </a>

                        <a class="" href="/brandProducts/{{firstB}}">
                            <h4>SHOP BY BRANDS</h4>
                        </a>


                    </div>
                    {{!-- Categories --}}
                    <div class="tab-content jump" id="mens">
                        <div class="row">
                            <div class="rhs_product_tab rhs_match_height">
                                {{#each products}}
                                <div class="col-xl-3 col-lg-4 col-md-6 col-sm-6 col-xs-12 mb-4 ">

                                    <div class="single-product mb-35">
                                        <div class="product-img">
                                            <a href="/product/{{this._id}}">
                                                <img src="https://shabith.ml/productImages/{{this._id}}a.jpg" alt="">
                                            </a>
                                            <div class="product-action">
                                                <a class="animate-left" href="" id="{{this._id}}A"
                                                    onclick="addToWishlistA('{{this._id}}',event)" title="Wishlist">
                                                    <i class="ion-ios-heart-outline"></i>
                                                </a>
                                            </div>
                                        </div>
                                        <div class="product-content">
                                            <div class="product-title-price">
                                                <div class="product-title">
                                                    <h4><a href="#">{{this.name}}</a>
                                                    </h4>
                                                </div>
                                            </div>

                                            <div class="product-cart-categori">

                                                <div class="product-cart">
                                                    <span>{{this.category}}</span>
                                                </div>
                                                <div class="product-price">

                                                    {{#if this.offer}}
                                                    <div class="product-price">
                                                        <span class="price--sale"><span class=money>₹
                                                                {{this.price}}</span></span>
                                                        <span><del><span class=money>₹
                                                                    {{this.actualPrice}}</span></del></span>

                                                    </div>
                                                    {{else}}
                                                    <div class="product-price">
                                                        <span class="price--sale"><span class=money>₹
                                                                {{this.price}}</span></span>
                                                    </div>
                                                    {{/if}}

                                                </div>

                                                <div class="product-categori ">

                                                    {{#if this.stockout}}
                                                    <span class="text-danger">Out of stock</span>
                                                    {{else}}
                                                    <button class="add_cart btn btn-light cart-item "
                                                        style="width: 130px; border: 1px solid black !important;"
                                                        onclick="addToCart('{{this._id}}','{{this.stock}}')"
                                                        title="Add to cart">

                                                        <i class="ion-bag"></i>
                                                        <i class="ion-ios-loop-strong animated rotateIn infinite"></i>
                                                        <i class="ion-android-done"></i> Add to Cart
                                                    </button>
                                                    <br>
                                                    <a class="add_cart btn btn-light cart-item mt-1"
                                                        href="/buyNow/{{this._id}}"
                                                        style="width: 130px; border: 1px solid black !important;">

                                                        <i class="ion-bag"></i>
                                                        <i class="ion-ios-loop-strong animated rotateIn infinite"></i>
                                                        <i class="ion-android-done"></i> Buy Now
                                                    </a>
                                                    {{/if}}

                                                </div>

                                            </div>
                                        </div>
                                    </div>
                                </div>
                                {{/each}}
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <style>
        .product-area.neha_prod_area {
            padding-top: 95px;
            padding-bottom: 60px;
            background-color: #fff;
        }
    </style>
    </div>
